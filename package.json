{
  "name": "huffman-javascript",
  "version": "0.1.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "dev": "concurrently \"tsc --watch src/__run.ts --outDir ./temp\" \"nodemon ./temp/__run.js\"",
    "build": "npx rollup -c",
    "lint": "eslint --ext .js,.ts, src --color",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss,css,json}\" && eslint \"src/**/*.{scss,css,json}\" --fix"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.34",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "rollup": "^2.41.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "typescript": "^4.2.3"
  }
}
